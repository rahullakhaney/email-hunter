              <div class="row">
                <form id="email-form" class="col s12 l8 push-l2">

                  <div class="row">
                    <div class="input-field col s6">
                      <input placeholder="Rahul" id="first_name" name="first_name" type="text" class="validate" data-error="Please enter a first name">
